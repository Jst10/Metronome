<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomLight">
        <attr name="type" format="enum" >
            <enum name="main" value="0"/>
            <enum name="minor" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomProgressBar">
        <attr name="orientation" format="enum" >
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="direction" format="enum" >
            <enum name="positive" value="0"/>
            <enum name="negative" value="1"/>
        </attr>
        <attr name="progressColor" format="color"/>
        <attr name="noProgressColor" format="color"/>
        <attr name="borderColor" format="color"/>
        <attr name="allBorderWidth" format="dimension"/>
        <attr name="maxProgress" format="integer"/>

    </declare-styleable>
</resources>